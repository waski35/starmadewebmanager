{% extends 'base.html.twig' %}

{% block body %}
    <div id="page-wrapper" style="min-height: 650px;">
            <div class="row">
                <div class="col-lg-12">
                        <h1 class="page-header">Sector Logs</h1>
                </div>
                    <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
            <table class="table" id="sectorsLog">
               <thead>
                   
                    <th>line</th>
                    <th>last update</th>
                    <th>name</th>
                    <th>peace</th>
                    <th>protected</th>
                    <th>type</th>
               </thead>
               <tbody>
                   
               </tbody>
               
           </table>
            </div>
        </div>       
    <script>
        $(document).ready(function() {
                $('#sectorsLog').DataTable({
                    "bServerSide": true,
                    "sAjaxSource" : '/admin/sector/listAjax',
                    "sServerMethod": "POST"
                    },
                    "aoColumns": [
                        { "mData": 'line', 
                            "bSearchable": true,
                            "bSortable": true},
                        { "mData": 'LASTUPDATE',
                            "bSearchable": true,
                            "bSortable": true},
                        { "mData": 'NAME',
                            "bSearchable": true,
                            "bSortable": true},
                        { "mData": 'PEACE',
                            "bSearchable": true,
                            "bSortable": true},
                        { "mData": 'PROTECTED',
                            "bSearchable": true,
                            "bSortable": true},
                        { "mData": 'TYPE',
                            "bSearchable": true,
                            "bSortable": true},
                    ]
                });});
        
        
            
    </script>
{% endblock %}

{% block stylesheets %}

{% endblock %}
